import java.util.Scanner;
public class Main
{
    public static void main (String[] args) {
        double saldo = 1000;
        Scanner ler = new Scanner(System.in);
        
        System.out.println("Escolha uma opção");
        System.out.println("1- Sacar");
        System.out.println("2- Depositar");
        System.out.println("3- Ver saldo");
        int opcao = ler.nextInt();
        
        switch (opcao){
            case 1:
                System.out.println("Informe quanto deseja Sacar");
                double saque = ler.nextdouble();
                
                float valorSaque = saldo - saque;
                System.out.format("Seu Saldo é:", valorSaque);
                break;
            case 2:
                System.out.println("Informe quanto deseja Depositar");
                float deposito = ler.nextInt();
                double valorDeposito = deposito + saldo;
                System.out.println("Saldo é", saldo);
                
            case 3:
                System.out.println("Saldo atual é", saldo);
            default :
                System.out.println("operação inválida");
                
        }
        
    
        
        
    }
}

